#!/usr/bin/env bash

mkdir tests temp log
for d in {9999..9999}; do
	mkdir -p $d
	/usr/bin/time -v                            \
	        cal_apriori_pang_uvfits.py          \
	        --caldir $METADIR/SEFD_${BAND^^}/$d \
	        --outdir $d $SRCDIR/$d              \
		>  log/apriori-$d.log               \
		2> log/apriori-$d.err               &
done
wait $(jobs -p)
